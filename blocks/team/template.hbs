<!-- Team -->
	<div id="{{block_id}}" class="section md-padding">

		<!-- Container -->
		<div class="container">

			<!-- Row -->
			<div class="row">

				<!-- Section header -->
				<div class="section-header text-center">
					<h2 class="title">{{header}}</h2>
				</div>
				<!-- /Section header -->
{{#each_with_sort team "order"}}
				<!-- team -->
				<div class="col-sm-4">
					<div class="team">
						<div class="team-img">
							<img class="img-responsive" src="{{image}}" alt="">
							<div class="overlay">
								<div class="team-social">
									<a href="{{link_fa}}"><i class="{{icon_fa}}"></i></a>
									<a href="{{link_go}}"><i class="{{icon_go}}"></i></a>
									<a href="{{link_twit}}"><i class="{{icon_twit}}"></i></a>
								</div>
							</div>
						</div>
						<div class="team-content">
							<h3>{{title}}</h3>
							<span>{{text}}</span>
						</div>
					</div>
				</div>
				<!-- /team -->
{{/each_with_sort}}
			</div>
			<!-- /Row -->

		</div>
		<!-- /Container -->

	</div>
	<!-- /Team -->