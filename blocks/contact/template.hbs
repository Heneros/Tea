
 <script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js"></script>

	<!-- Contact -->
	<div id="{{block_id}}" class="section md-padding">

		<!-- Container -->
		<div class="container">

			<!-- Row -->
			<div class="row">

				<!-- Section-header -->
				<div class="section-header text-center">
					<h2 class="title">{{header}}</h2>
				</div>
				<!-- /Section-header -->
{{#each_with_sort contact "order"}}
				<!-- contact -->
				<div class="col-sm-4">
					<div class="contact">
						<i class="{{icon}}"></i>
						<h3>{{title}}</h3>
						<p>{{text}}</p>
					</div>
				</div>
				<!-- /contact -->
{{/each_with_sort}}

				<!-- contact form -->
				<div class="col-md-8 col-md-offset-2">
					<form action="" name="myform" id="myform" class="contact-form">
						<input type="text" class="input" id="name" name="name" placeholder="Name" value="">
           <input type="text" class="input" id="email" name="user_email"  size="30" value="" value=""/> 
						<input type="text" class="input" name="user_subject" placeholder="Subject" value="">
						<textarea class="input" name="textarea" placeholder="Message" value=""></textarea>
						<button  type="submit" name="submit" value="Submit" class="main-btn">{{btn}}</button>
					</form>
					   <div id="results"><div></div>
				</div>
				<!-- /contact form -->

			</div>
			<!-- /Row -->


		</div>
		<!-- /Container -->

	</div>
	<!-- /Contact -->


 <script>
$(".submit-button").on("click", function (event) {
        event.preventDefault();
        var name = $("#name").val();
        var email = $("#email").val();
        $.ajax({
            url: "/wp-admin/admin-ajax.php",
            method: 'post',
            data: {
                action: 'ajax_order',
                name: name,
                email: email

            },
            success: function (response) {
                $('.main-btn').html(response);
            }
        });
    });

</script>	